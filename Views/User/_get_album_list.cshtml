@model IEnumerable<ViewModels.FileDataMemberViewModel>

@if (Model.Count() > 0)
{
    foreach (var data in Model)
    {
        <li>
            <a href='@ViewBag.URL&album=@data.album_id'>
                <img src="~/Content/assets/icons/album-bg.png" class='MemberPhotos img-responsive' alt='' />
                <span class='overText'>@data.name (@data.phCount Photos)<br /> @data.strDate </span>
            </a>
        </li>
    }
}