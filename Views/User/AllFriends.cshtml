@model IEnumerable<ViewModels.FriendRequestViewModel>


<div class="clearfix"></div>

<div class="row">

    <div class="col-lg-12">

        <div class="mutualFriends row">
            @if (Model.Count() > 0)
            {

                foreach (var item in Model)
                {
                    <div class="col-sm-4 userInfoLog">
                        <div class="memberBox">
                            <div class="MemberPhotos">
                                @if (@item.Mem_Pic == null)
                                {
                                    <span data-user-id="@item.Mem_ID" data-user-img="../Content/images/default-profile.png" data-user-name="@item.Mem_Name">
                                        <img src="~/Content/images/default-profile.png" class="img-responsive" />
                                    </span>
                                }
                                else
                                {
                                    <span data-user-id="@item.Mem_ID" data-user-img="@item.Mem_Pic" data-user-name="@item.Mem_Name">
                                        <img src="@item.Mem_Pic" class="img-responsive" />
                                    </span>
                                }
                            </div>
                            <div class="memberDetail marginT15px text-center">
                                <p>@item.Mem_Name<br /><span class="graycolor"><small>@item.Mem_Email</small></span></p>
                                <p class="logButtons memberBtnBox">
                                    <a id="btnaccept" href="#" onclick='UnFriendRequest(@item.RequestID,this)' class="btn btn-primary">Unfriend</a>
                                </p>
                            </div>
                        </div>                                                
                    </div>
                }
            }
            else
            {

            }
        </div>
    </div>
</div>
