@model IEnumerable<ViewModels.BusinessCommunityViewModel>


<select class="form-control" name="memComm" id="MemberComm">
    <option value="-1">--Select Community--</option>
    @if (Model.Count() > 0)
    {
        foreach (var data in Model)
        {
            <option value="@data.CommunityId" @(@ViewBag.commId==@data.CommunityId?"selected":"")>@data.BusinessCommunity</option>
        }
    }
</select>

