@model IEnumerable<ViewModels.Products.FilterNameViewModel>

<h4>
    Filters
    <span class="pull-right">
        Clear All
    </span>
</h4>
<div class="filterBox marginTop30px">
    @if (Model.Count() > 0)
    {
        foreach (var data in Model)
        {
            <div class="widget filter">
                <h5>@data.FilterName</h5>
                @Html.Action("_getFilterValues", "shop", new { FilterId=data.FilterId,categoryId = ViewBag.catId, catType = ViewBag.catType })
                
            </div>
        }
    }
   
</div>
